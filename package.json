{
  "name": "brotli-cli",
  "version": "1.0.2",
  "repository": "git@github.com:dzek69/brotli-cli.git",
  "author": "Jacek Nowacki",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "docs": "typedoc src/index.ts --out docs --listInvalidSymbolLinks --includes tutorials --theme pages-plugin --includeVersion",
    "compile": "npm run compile:cjs",
    "compile:cjs": "del-cli dist && tsc --project tsconfig.cjs.json && node ./build-scripts/compile.cjs.after.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:fix": "npm run lint --fix",
    "prepack": "npm run compile",
    "prepublishOnly": "npm audit && npm run lint && npm run test && npm run docs",
    "start:dev": "nodemon",
    "start:dev:compatibility": "TS_NODE_FILES=true npm run start:dev"
  },
  "bin": "./dist/index.js",
  "type": "module",
  "dependencies": {
    "better-custom-error": "^4.0.1",
    "brotli": "^1.3.2",
    "fast-glob": "^3.2.5",
    "yargs": "^17.0.2-candidate"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@dzek69/eslint-config-base": "^2.0.0",
    "@dzek69/eslint-config-typescript": "^0.4.0",
    "@types/brotli": "^1.3.0",
    "@types/jest": "^26.0.23",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-plugin-module-extension": "^0.1.3",
    "del-cli": "^4.0.0",
    "eslint": "^7.14.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "jest": "^27.0.6",
    "must": "^0.13.4",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typedoc": "^0.21.2",
    "typedoc-plugin-pages-fork-fork": "^0.0.3",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepublishOnly && npm run compile"
    }
  },
  "libraryTemplate": {
    "version": "3.3.1",
    "language": "typescript",
    "fixDefaultForCommonJS": true,
    "jsx": false
  }
}
